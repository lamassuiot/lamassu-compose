{
    "datacenter": "lamassu-dc",
    "log_level": "INFO",
    "node_name": "consul-server",
    "server": true,
    "ui_config": {
        "enabled": true
    },
    "addresses": {
        "https": "0.0.0.0"
    },
    "ports": {
        "https": 8501
    },
    "ca_file": "/consul/tls/internal-ca.crt",
